* GLOBAL:
    TO_FILE = true
    TO_STANDARD_OUTPUT   = true
    LOG_FLUSH_THRESHOLD  = 1
* INFO:
    TO_STANDARD_OUTPUT   = false
    TO_FILE         = true
* TRACE:
    TO_FILE           = false
    TO_STANDARD_OUTPUT   = true
* DEBUG:
    TO_FILE          = true
    TO_STANDARD_OUTPUT   = false
* WARNING:
    TO_STANDARD_OUTPUT   = true
    TO_FILE         = false
* ERROR:
    TO_STANDARD_OUTPUT   = true
    TO_FILE         = true
* FATAL:
    TO_STANDARD_OUTPUT   = true
    TO_FILE              = true
